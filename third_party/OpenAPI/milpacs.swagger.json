            }
          }
        },
        "parameters": [
          {
            "name": "username",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "userId",
            "in": "query",
            "required": false,
            "type": "integer",
            "format": "int32"
          }
        ],
        "tags": [
          "Users, Milpacs, Profile"
        ]
      }
    },
    "/api/v1/roster/{roster}": {
      "get": {
        "summary": "Get a given roster",
        "description": "Get all milpac Profile data for a given roster",
        "operationId": "Milpacs_GetRoster",
        "responses": {
          "200": {
            "description": "A successful response.",
