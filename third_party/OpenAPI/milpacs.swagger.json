        },
        "parameters": [
          {
            "name": "roster",
            "in": "path",
            "required": true,
            "type": "string",
            "enum": [
              "COMBAT",
              "ON_ELOA",
              "RESERVE",
              "PAST_MEMBERS",
              "WALL_OF_HONOR"
            ]
          }
        ],
        "tags": [
          "Roster, Milpacs"
        ]
      }
    }
  },
